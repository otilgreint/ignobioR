library(terra)
# Load existing rasters
ndvi <- rast("inst/extdata/ndvi_example.tif")
mrfi <- rast("inst/extdata/mrfi_example.tif")
dem <- rast("inst/extdata/dem_example.tif")
# Target CRS (use MRFI's CRS as standard)
target_crs <- "EPSG:3035"
# Reproject NDVI and DEM to match MRFI
ndvi_reproj <- project(ndvi, target_crs, method = "bilinear")
dem_reproj <- project(dem, target_crs, method = "bilinear")
# Optional: Resample to same resolution if needed
# (MRFI is much coarser: 35x17 vs 408x213)
# You might want to keep original resolutions though
# Save corrected versions
writeRaster(ndvi_reproj, "inst/extdata/ndvi_example.tif", overwrite = TRUE)
writeRaster(dem_reproj, "inst/extdata/dem_example.tif", overwrite = TRUE)
# MRFI already correct, no need to resave
# Verify they now match
cat("After correction:\n")
cat("NDVI CRS:", crs(ndvi_reproj, describe = TRUE)$name, "\n")
cat("MRFI CRS:", crs(mrfi, describe = TRUE)$name, "\n")
cat("DEM CRS:", crs(dem_reproj, describe = TRUE)$name, "\n")
